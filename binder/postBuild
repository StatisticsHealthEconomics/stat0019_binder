wget http://pj.freefaculty.org/Ubuntu/15.04/amd64/openbugs/openbugs_3.2.3.orig.tar.gz
tar -zxvf openbugs_3.2.3.orig.tar.gz 
cd openbugs-3.2.3 
./configure && make && make install 


